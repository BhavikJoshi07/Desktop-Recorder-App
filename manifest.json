{
  "manifest_version": 2,

  "name": "Desktop Recorder",
  "description": "Allows you to record the screen.",
  "version": "1.0",
  "app" : {
    "background" : {
      "scripts" : ["jquery.js","background.js"]
    }
  },
  "icons" : {
    "16" : "icon.png",
    "64" : "icon.png",
    "128" : "icon.png"
  },
  "permissions": [
    "desktopCapture"
  ]
}
